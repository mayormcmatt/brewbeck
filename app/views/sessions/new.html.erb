<h2>Sign In</h2>

<div class="sign_in_form">
    <%= form_for(:session, url: sessions_path) do |f| %>
        <%= f.text_field :email, placeholder: "Email" %><br>
        <%= f.password_field :password, placeholder: "Password" %><br>
        <%= f.submit "Sign In" %>
    <% end %>
    
    <a href="/users/new">New Users Sign Up Here</a>
</div>    